{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 9 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "~1.0",
        "drupal/acquia_connector": "^4.0",
        "drupal/address": "^1.9",
        "drupal/admin_toolbar": "^3.0",
        "drupal/adminimal_theme": "^1.6",
        "drupal/allowed_formats": "^2.0.0",
        "drupal/asset_injector": "^2.7",
        "drupal/back_to_top": "^3.0.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/block_class": "^2.0",
        "drupal/block_field": "^1.0@RC",
        "drupal/bootstrap": "^3.23",
        "drupal/bootstrap5": "^3.0",
        "drupal/civic_accessibility_toolbar": "^1.0",
        "drupal/composer_deploy": "^1.7",
        "drupal/computed_field": "^4.0@alpha",
        "drupal/config_filter": "^2.6",
        "drupal/config_ignore": "^3.1",
        "drupal/config_split": "^2.0",
        "drupal/core-composer-scaffold": "^10.0.2",
        "drupal/core-project-message": "^10.0.2",
        "drupal/core-recommended": "^9",
        "drupal/core-vendor-hardening": "^10.0.2",
        "drupal/crop": "^2.1",
        "drupal/ctools": "^4.0.3",
        "drupal/devel": "^5.0",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/devel_php": "^1.3",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/embed": "^1.4",
        "drupal/entity_browser": "^2.6",
        "drupal/entity_browser_entity_form": "^2.9.0",
        "drupal/entity_embed": "^1.2",
        "drupal/entity_print": "^2.6",
        "drupal/entity_update": "^3.0",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/eu_cookie_compliance": "^1.14",
        "drupal/fancy_file_delete": "2.0.x-dev",
        "drupal/faqfield": "^7.0",
        "drupal/feeds": "^3.0@alpha",
        "drupal/feeds_ex": "^1.0@beta",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_group": "^3.1",
        "drupal/field_permissions": "^1.1",
        "drupal/field_states_ui": "^3.0",
        "drupal/flag": "^4.0@beta",
        "drupal/fontawesome": "^2.19",
        "drupal/geocoder": "*",
        "drupal/geofield": "^1.52",
        "drupal/geofield_map": "^3.0",
        "drupal/honeypot": "^2.1",
        "drupal/image_styles_mapping": "^1.5",
        "drupal/image_widget_crop": "^2.3",
        "drupal/inline_entity_form": "^2.0@RC",
        "drupal/insert_view": "^2.0",
        "drupal/jquery_ui_effects": "^2.0",
        "drupal/jsonapi_extras": "^3.20",
        "drupal/key": "^1.15",
        "drupal/leaflet": "^10.0.6",
        "drupal/leaflet_more_maps": "2.1.x-dev@dev",
        "drupal/link_attributes": "^2.1",
        "drupal/linkit": "^6.0@beta",
        "drupal/login_destination": "^2.0@beta",
        "drupal/loqate": "^3.0",
        "drupal/mailsystem": "^4.3",
        "drupal/media_entity_browser": "^2.0@alpha",
        "drupal/media_entity_download": "^2.1",
        "drupal/media_library_edit": "^3.0.2",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/metatag": "^2.0",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/migrate_upgrade": "^4.0",
        "drupal/paragraphs": "^1.13",
        "drupal/paragraphs_entity_embed": "^3.0",
        "drupal/paragraphs_viewmode": "^1.0",
        "drupal/pathauto": "^1.8",
        "drupal/queue_ui": "^3.1",
        "drupal/quick_node_clone": "^1.14",
        "drupal/realname": "^2.0@beta",
        "drupal/redirect": "^1.6",
        "drupal/registration_role": "^2.0",
        "drupal/responsive_menu": "^4.4",
        "drupal/restui": "^1.20",
        "drupal/sendgrid_integration": "^2.2",
        "drupal/shield": "^1.4",
        "drupal/slick": "^2.3",
        "drupal/smtp": "^1.0",
        "drupal/stage_file_proxy": "^2.0.2",
        "drupal/super_login": "^2.0",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/tb_megamenu": "^2.0@alpha",
        "drupal/term_condition": "^2.0",
        "drupal/theme_switcher": "^2.0",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.1",
        "drupal/upgrade_status": "^4.0",
        "drupal/uuid_extra": "^2.0",
        "drupal/views_argument_token": "^2.0",
        "drupal/views_bulk_operations": "^4.0",
        "drupal/views_combine": "^1.0@alpha",
        "drupal/views_display_union": "^1.0",
        "drupal/views_entity_embed": "^2.0",
        "drupal/views_exclude_previous": "^2.0@beta",
        "drupal/views_geojson": "^1.0",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/webform": "^6.0",
        "drush/drush": "^10.2",
        "geocoder-php/google-maps-provider": "^4.6",
        "mikehaertl/phpwkhtmltopdf": "~2.1.",
        "tecnickcom/tcpdf": "~6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
          "composer/installers": true,
          "cweagans/composer-patches": true,
          "drupal/core-composer-scaffold": true,
          "drupal/core-project-message": true,
          "drupal/core-vendor-hardening": true,
          "dealerdirect/phpcodesniffer-composer-installer": true,
          "php-http/discovery": true
      }
    },
    "extra": {
        "patches": {
            "drupal/responsive_menu": {
                "Gestures on desktop patch": "https://www.drupal.org/files/issues/2021-05-27/3208962-3.patch"
            },
            "drupal/core": {
                "Add template suggestions to blocks patch": "https://www.drupal.org/files/issues/2020-07-02/3062376-34.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            },
            "file-mapping": {
              "[web-root]/google123.html": false,
              "[web-root]/web.config": false,
              "[web-root]/robots.txt": false,
              "[web-root]/.ht.router.php": false,
              "[web-root]/.htaccess": false,
              "[web-root]/.eslintignore": false,
              "[web-root]/.eslintrc.json": false,
              "[web-root]/.editorconfig": false,
              "[web-root]/.csslintrc": false,
              "[web-root]/.gitattributes": false,
              "[web-root]/sites/development.services.yml": false
          }
        },
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
